<?xml version="1.0" encoding="utf-8"?>
<Include xmlns:xi="http://www.w3.org/2001/XInclude">
  <!-- Arctic Circle lines -->
  <Style filter-mode="first" name="ne10mgeographiclines">
    <Rule>
      <Filter>([name] = 'Arctic Circle')</Filter>
      <LineSymbolizer clip="false" stroke="#116688" stroke-opacity="0.5" stroke-width="1"/>
      <TextSymbolizer clip="false" fontset-name="fontset-0" halo-radius="2" placement="line" spacing="800"><![CDATA[[name]]]></TextSymbolizer>
    </Rule>
  </Style>
  <Layer name="ne10mgeographiclines" srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs">
    <StyleName>ne10mgeographiclines</StyleName>
    <Datasource>
      <Parameter name="file"><![CDATA[data/ne_10m_geographic_lines/ne_10m_geographic_lines.shp]]></Parameter>
      <Parameter name="type"><![CDATA[shape]]></Parameter>
    </Datasource>
  </Layer>
  <!-- Graticule lines -->
  <Style filter-mode="first" name="10mgraticules15">
    <Rule>
      <Filter>([display] = '45 N')</Filter>
      <LineSymbolizer clip="false" stroke="#116688" stroke-opacity="0.5" stroke-width="1"/>
      <TextSymbolizer clip="false" fontset-name="fontset-0" halo-radius="2" placement="line" spacing="800"><![CDATA[[display]]]></TextSymbolizer>
    </Rule>
  </Style>
  <Layer name="10mgraticules15" srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs">
    <StyleName>10mgraticules15</StyleName>
    <Datasource>
      <Parameter name="file"><![CDATA[data/ne_10m_graticules_15/ne_10m_graticules_15.shp]]></Parameter>
      <Parameter name="type"><![CDATA[shape]]></Parameter>
    </Datasource>
  </Layer>
</Include>
